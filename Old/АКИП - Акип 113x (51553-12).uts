# Функция для выбора поверяемого прибора
Function MODE_PRIBOR_CHOICE
message mem_1 selectmenu defvalue=1 "Выберите поверяемый прибор:\n 1. АКИП-1133 \n 2. АКИП-1133A \n 3. АКИП-1134 \n 4. АКИП-1134A \n 5. АКИП-1135 \n 6. АКИП-1135A "
    CaseOne
        when mem_1 = 1
        call MODE_PRIBOR_CHOICE1
        when mem_1 = 2
        call MODE_PRIBOR_CHOICE2
        when mem_1 = 3
        call MODE_PRIBOR_CHOICE3
        when mem_1 = 4
        call MODE_PRIBOR_CHOICE4
        when mem_1 = 5
        call MODE_PRIBOR_CHOICE5
        when mem_1 = 6
        call MODE_PRIBOR_CHOICE6
    EndCase

    define MeasureVDC                        false                        #    Наличие пункта поверки внутреннего вольтметра
    define MeasureIDC                        false                        #    Наличие пункта поверки внутреннего амперметра
    define METODIKA_POVERKI                 МП 51553-12            #    Методика поверки
    define METODIKA_OSMOTR                     7.2                            #    № пункта для "Внешнего осмотра"
    define METODIKA_OPROB                     7.5                            #    № пункта для "Опробования"
    define CHANEL_NOM                        1                            #    Количество каналов
    define U_pit                            220                            #    Номинальное напряжение питания прибора
    define RegNum                            51553-12                    #    Номер в Госреестре
    report METPOV        "METODIKA_POVERKI"
    report Num        "RegNum"
    report Imax        "I_max А"
    report Umax        "U_max В"
    report Si        "Источник питания постоянного тока Model_SI_Report"
EndFunction



