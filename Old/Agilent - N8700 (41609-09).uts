# Поверяемый прибор: Источники питания постоянного тока Agilent/Keysight серии N8700
# Поверяемые параметры: (VDC), (IDC), (PVDC), (PIDC), (INSTB_U_Upit), (INSTB_I-Upit), (INSTB_U_Inagr), (INSTB_I_Unagr)
# 	 
#
# Поверяемые параметры: 
# 		погрешность формирования выходного напряжения (VDC) и силы выходного тока (IDC)
# 		пульсации выходного напряжения (PVDC) и выходного тока (PIDC)
# 		нестабильности при изменении напряжения питающей сети выходного напряжения (INSTB_U_Upit) и тока (INSTB_I_Upit) 
# 		нестабильность выходного напряжения при изменении тока нагрузки (INSTB_U_Inagr)
# 		нестабильность выходного тока при изменении выходного напряжения (INSTB_I_Unagr)   
# 		
#
####################################################################################################################################################### 
###############################################################  S   T   A   R   T   ##################################################################
#######################################################################################################################################################
#
#/ Скрипт для поверки приборов Agilent/Keysight серии N8700
#/<-

#	Выбор поверяемого прибора
call MODE_PRIBOR_CHOICE

#	Выбор нагрузки
call LOAD_CHOISE

#	Выбор типа поверки
call MODE_POVERKA_CHOICE

#	Установка стандартных значений для данного скрипта  (DEFINE)
call START_DEFINE

#	Данные для отчета:
call MODE_INPUT_TABLE

#	Внешний осмотр поверяемого устройства
Call VISUAL_CHECK

#	Опробование устройства
Call OPROBIROVANIE


#####################################################################################################################################################################
##############################################################   D   O   T   S   ####################################################################################
#####################################################################################################################################################################

##################################################################################################################################################################
#/	Определение абсолютной погрешности формирования выходного постоянного напряжения. 		
##################################################################################################################################################################

#						Напряжение СИ от U_max, В;		Ток СИ от Imax, А;		Ток нагрузки от Imax, А;	Интервал жидания, с;	
call VDC						0,9								1						0							2k
call VDC						0,5								1						0							2k
call VDC						0,1								1						0							2k

###################################################################################################################################################################
#/	Определение нестабильности выходного напряжения при изменении напряжения питающей сети.
###################################################################################################################################################################

#						Напряжение СИ от U_max, В;		Ток СИ от Imax, А;		Ток нагрузки от Imax, А;	Интервал жидания, с;	Напряжение питания от U_pit, В;	Количесво измерений;	
call INSTB_U_Upit_povt    	0,1								1						0							2k						1,1										5
call INSTB_U_Upit_povt		0,1								1						0							2k						0,9										5
call INSTB_U_Upit_povt    	0,5								1						0							2k						1,1										5
call INSTB_U_Upit_povt		0,5								1						0							2k						0,9										5
call INSTB_U_Upit_povt    	0,9								1						0							2k						1,1										5
call INSTB_U_Upit_povt		0,9								1						0							2k						0,9										5

###################################################################################################################################################################
#/	Определение нестабильности выходного напряжения при изменении тока нагрузки. 			
###################################################################################################################################################################

#						Напряжение СИ от U_max, В;		Ток СИ от Imax, А;		Ток нагрузки от Imax, А;	Интервал жидания, с;	Выходной ток от I_max 2, В;	Количесво измерений;
call INSTB_U_Inagr_povt		0,1								1						0,9							2k                     0,1										5
call INSTB_U_Inagr_povt		0,1								1						0,9							2k                     0,5										5
call INSTB_U_Inagr_povt		0,5								1						0,9							2k                     0,1										5
call INSTB_U_Inagr_povt		0,5								1						0,9							2k                     0,5										5
call INSTB_U_Inagr_povt		0,9								1						0,9							2k                     0,1										5
call INSTB_U_Inagr_povt		0,9								1						0,9							2k                     0,5										5

###################################################################################################################################################################
#/	Определение пульсации выходного напряжения. 			
###################################################################################################################################################################

#						Напряжение СИ от U_max, В;							Ток нагрузки от Imax, А;	Интервал жидания, с;
call PVDC   					0,1													0,9							2k
call PVDC   					0,1													0							2k
call PVDC   					0,5													0,9							2k
call PVDC   					0,5													0							2k
call PVDC   					0,9													0,9							2k
call PVDC   					0,9													0							2k

###################################################################################################################################################################
#/	Определение абсолютной погрешности формирования силы выходного тока. 			
###################################################################################################################################################################

#						Ток СИ от Imax, А;		Напряжение СИ от U_max, В;		Ток нагрузки от Imax, А;	Интервал жидания, с;	
call IDC						0,9								1						1,1							2k
call IDC						0,5								1						1,1							2k
call IDC						0,1								1						1,1							2k

###################################################################################################################################################################
#/ Определение нестабильности выходного тока при изменении напряжения питающей сети. 			
###################################################################################################################################################################

#						Ток СИ от Imax, А;		Напряжение СИ от U_max, В; 		Ток нагрузки от Imax, А;	Интервал жидания, с;	Напряжение питания от U_pit, В;	Количесво измерений
call INSTB_I_Upit_povt     	0,9									1						1,1							2k 	            		1,1										5
call INSTB_I_Upit_povt 		0,9									1						1,1							2k 	            		0,9										5
call INSTB_I_Upit_povt     	0,5									1						1,1							2k 	            		1,1										5
call INSTB_I_Upit_povt 		0,5									1						1,1							2k 	            		0,9										5
call INSTB_I_Upit_povt     	0,1									1						1,1							2k 	            		1,1										5
call INSTB_I_Upit_povt 		0,1									1						1,1							2k 	            		0,9										5

###################################################################################################################################################################
#/	Определение нестабильности выходного тока при изменении выходного напряжения.			
###################################################################################################################################################################

#						Ток СИ от Imax, А;		Напряжение СИ от U_max, В;		Ток нагрузки от Imax, А;	Интервал жидания, с;	Выходное напряжение от U_max 2, В;	Количесво измерений
call INSTB_I_Unagr_povt    	0,9									0,1						1,1							2k 	            		1										5
call INSTB_I_Unagr_povt    	0,5									0,1						1,1							2k 	            		1										5
call INSTB_I_Unagr_povt    	0,1									0,1						1,1							2k 	            		1										5

###################################################################################################################################################################
#/	Определение пульсации выходного тока.											#uhesh(PIDC) 			
###################################################################################################################################################################

#						Ток СИ от Imax, А;		Напряжение СИ от U_max, В;		Ток нагрузки от Imax, А;	Интервал жидания, с;
call PIDC   				0,9									1						0,1							2k
call PIDC   				0,9									1						0,8							2k
call PIDC   				0,5									1						0,1							2k
call PIDC   				0,5									1						0,4							2k
call PIDC   				0,1									1						0,01						2k
call PIDC   				0,1									1						0,01						2k

#########################################################################################################################################################################
#########   E   N   D   ###############################################################################################################################
#########################################################################################################################################################################

# Функция сброса всех приборов в изначальное состояние
call RESET_ALL
     if TestResult
          report result "годным"
     else
          report result "не годным"
     endif

EndScript  


