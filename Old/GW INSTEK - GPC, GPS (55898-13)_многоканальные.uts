# Функция выбора типа поверки
function MODE_POVERKA_CHOICE
message mem_1 selectmenu defvalue=2 "Выберите тип поверки :\n 1. Первичная \n 2. Периодическая" 
	CaseOne
	when	mem_1 = 1
		CaseOne
		when CHANEL_NOM = 2
			Tree Disable VDC_CH3; INSTB_U_Upit_CH3; INSTB_U_Inagr_CH3; PVDC_CH3;
			Tree Disable VDC_CH4; INSTB_U_Upit_CH4; INSTB_U_Inagr_CH4; PVDC_CH4;
			Tree Enable	PVDC_CH1; PVDC_CH2
			Tree Enable	PIDC_CH1; PIDC_CH2
		when CHANEL_NOM = 3
			Tree Enable VDC_CH3; INSTB_U_Upit_CH3; INSTB_U_Inagr_CH3; 
			Tree Disable VDC_CH4; INSTB_U_Upit_CH4; INSTB_U_Inagr_CH4; PVDC_CH4;
			Tree Enable	PVDC_CH1; PVDC_CH2; PVDC_CH3
			Tree Enable	PIDC_CH1; PIDC_CH2; 
		when CHANEL_NOM = 4
			Tree Enable VDC_CH3; INSTB_U_Upit_CH3; INSTB_U_Inagr_CH3;
			Tree Enable VDC_CH4; INSTB_U_Upit_CH4; INSTB_U_Inagr_CH4;
			Tree Enable	PVDC_CH1; PVDC_CH2; PVDC_CH3; PVDC_CH4
			Tree Enable	PIDC_CH1; PIDC_CH2; 
		endcase
		report Vid_poverki	"Первичная"
	when	mem_1 = 2 
		CaseOne
		when CHANEL_NOM = 2
			Tree Disable VDC_CH3; INSTB_U_Upit_CH3; INSTB_U_Inagr_CH3; PVDC_CH3;
			Tree Disable VDC_CH4; INSTB_U_Upit_CH4; INSTB_U_Inagr_CH4; PVDC_CH4;
			Tree Disable	PVDC_CH1; PVDC_CH2
			Tree Disable	PIDC_CH1; PIDC_CH2
		when CHANEL_NOM = 3
			Tree Enable VDC_CH3; INSTB_U_Upit_CH3; INSTB_U_Inagr_CH3; 
			Tree Disable VDC_CH4; INSTB_U_Upit_CH4; INSTB_U_Inagr_CH4; PVDC_CH4; 
			Tree Disable	PVDC_CH1; PVDC_CH2; PVDC_CH3
			Tree Disable	PIDC_CH1; PIDC_CH2; 
		when CHANEL_NOM = 4
			Tree Enable VDC_CH3; INSTB_U_Upit_CH3; INSTB_U_Inagr_CH3;
			Tree Enable VDC_CH4; INSTB_U_Upit_CH4; INSTB_U_Inagr_CH4;
			Tree Disable	PVDC_CH1; PVDC_CH2; PVDC_CH3; PVDC_CH4
			Tree Disable	PIDC_CH1; PIDC_CH2; 
		endcase
	report Vid_poverki	"Периодическая"
	endcase
EndFunction

