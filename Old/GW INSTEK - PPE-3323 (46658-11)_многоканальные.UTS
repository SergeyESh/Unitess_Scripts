            define    MeasureIDC                    false                    #    Наличие поверки режима измерения тока

#    DriverSet  Model_SI_Driver RESET
#    Driverget  Model_SI_Driver mem_4 =  SERIAL                                #     Запрос номера модуля
#    DriverSet  Model_SI_Driver OUT = 1
    define METODIKA_POVERKI                     МП-221/447-2010                            #    Методика поверки
    define METODIKA_OSMOTR                         5.1                                            #    № пункта для "Внешнего осмотра"
    define METODIKA_OPROB                         5.2                                            #    № пункта для "Опробования"
    define U_pit                                220                                            #    Номинальное напряжение питания прибора
    define RegNum                                46658-11                                    #    Номер в Госреестре
    # Вывод данных в протокол
    report METPOV        "METODIKA_POVERKI"
    report modul_num    "mem_4"
    math mem_1=I_max*2
    math mem_2=U_max*2
    report Num                                    "RegNum"
    report Imax                                "Выходной ток: каналы 1(2): I_max А; канал 3: Ich3_max А; последовательное соединение каналов: I_max А"
