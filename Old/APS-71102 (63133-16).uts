message mem_1 selectmenu defvalue=1 "Выберите поверяемый прибор:\n 1.APS-71102" 
CaseOne
    when mem_1=1
	    define Model_SI_Driver 				InstekAPS					#	Модель поверяемого источника питания
    	define Model_SI_Report                 APS-71102                    #    Модель поверяемого источника питания
        define Interface                     true                        #    Наличие интерфейса у модели
        define U_max1                        140                            #    Максимальное выходное напряжение
        define U_max2                        280                            #    Максимальное выходное напряжение
        define I_max1                        10                            #    Максимальный выходной ток
        define I_max2                        5                            #    Максимальный выходной ток
        define F_max                            550                            #    Максимальный выходной ток
        define P_max                            1000                            #    Максимальный выходной ток
#        define K_pow                        1                            #    Максимальный выходной ток
#        define K_garm                        0.5                            #    Максимальный выходной ток
    # Напряжение    
        define VDC_coe                        0.005                        #    коэффициент для пересчета погрешности
        define VDC_con1                        0.8                            #    коэффициент для пересчета погрешности
        define VDC_con2                        1.6                            #    коэффициент для пересчета погрешности
        define VDCpit_coe                    0                            #    коэффициент для пересчета погрешности
        define VDCpit_con                    0.56                            #    коэффициент для пересчета погрешности
        define VDCnagr_coe                    0                        #    коэффициент для пересчета погрешности
        define VDCnagr_con1                    0.7                            #    коэффициент для пересчета погрешности
        define VDCnagr_con2                    1.4                            #    коэффициент для пересчета погрешности
    # Ток    
        define IDCizm_coe1                    0                        #    коэффициент для пересчета погрешности
        define IDCizm_con1                    0.15                        #    коэффициент для пересчета погрешности
        define IDCizm_coe2                    0                        #    коэффициент для пересчета погрешности
        define IDCizm_con2                    0.15                        #    коэффициент для пересчета погрешности
    # Мощность    
        define POWizm_coe                    0                        #    коэффициент для пересчета погрешности
        define POWizm_con                    24                            #    коэффициент для пересчета погрешности
#        define K_Powizm_coe                    0.02                        #    коэффициент для пересчета погрешности
#        define K_Powizm_con                    0,002                        #    коэффициент для пересчета погрешности
    # Частота
        define Freq_coe                        0                            #    коэффициент для пересчета погрешности
        define Freq_con                        0.1                            #    коэффициент для пересчета погрешности
EndCase
    
	Driverget  Model_SI_Driver mem_4 =  SERIAL							# 	Запрос номера модуля
    define METODIKA_POVERKI                 МП 63133-16            #    Методика поверки
    define METODIKA_OSMOTR                     7.2                            #    № пункта для "Внешнего осмотра"
    define METODIKA_OPROB                     7.4                            #    № пункта для "Опробования"
    define CHANEL_NOM                        1                            #    Количество каналов
    define U_pit                            220                            #    Напряжение питания прибора
    define RegNum                            63133-16                    #    Номер в Госреестре
    report METPOV        "METODIKA_POVERKI"
    report modul_num	"mem_4"
	report Num            "RegNum"
    report Imax            "Диапазон 1 - I_max1 А, диапазон 2 - I_max2 A"
    report Umax            "Диапазон 1 - U_max1 В, Диапазон 2 - U_max2 В"
    report Fr_max            "Частота - Freq_max, Гц"
    report Po_max            "Мощность - P_max, Вт"
    report Si            "Источник питания Model_SI_Report"
    
